{
	"name": "frackit",
	"version": "0.0.1",
	"description": "Opinionated sveltekit starter template.",
	"keywords": [
		"sveltekit",
		"template",
		"starter",
		"svelte",
		"kit"
	],
	"type": "module",
	"scripts": {
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"dev": "vite dev",
		"extras": "pnpm i -D type-fest froebel nanostores rxjs",
		"format": "prettier --write --plugin-search-dir=. .",
		"postinstall": "svelte-kit sync",
		"lint": "prettier --check . --plugin-search-dir . && eslint --ignore-path .gitignore .",
		"preview": "vite preview",
		"test": "playwright test",
		"test:unit": "vitest"
	},
	"devDependencies": {
		"@dimforge/rapier3d-compat": "^0.10.0",
		"@playwright/test": "^1.29.0",
		"@sveltejs/adapter-vercel": "1.0.0",
		"@sveltejs/kit": "1.0.1",
		"@theatre/core": "^0.5.0",
		"@theatre/studio": "^0.5.0",
		"@threlte/core": "^5.0.8",
		"@threlte/extras": "^4.9.2",
		"@threlte/preprocess": "^0.0.2",
		"@threlte/rapier": "^0.5.0",
		"@threlte/theatre": "^0.0.2",
		"@types/cookie": "^0.5.1",
		"@types/node": "18.11.16",
		"@types/three": "^0.146.0",
		"@typescript-eslint/eslint-plugin": "^5.46.1",
		"@typescript-eslint/parser": "^5.46.1",
		"autoprefixer": "10.4.13",
		"cookie": "^0.5.0",
		"cssnano": "5.1.14",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"fractils": "3.1.0",
		"greset": "2.0.0",
		"mdsvex": "0.10.6",
		"postcss": "8.4.20",
		"postcss-load-config": "4.0.1",
		"prettier": "2.8.1",
		"prettier-plugin-svelte": "2.9.0",
		"pug": "3.0.2",
		"rehype-autolink-headings": "6.1.1",
		"rehype-slug": "5.1.0",
		"remark-abbr": "1.4.1",
		"sass": "1.56.2",
		"svelte": "3.55.0",
		"svelte-check": "^2.10.2",
		"svelte-preprocess": "5.0.0",
		"svelte-sequential-preprocessor": "^0.0.7",
		"three": "^0.147.0",
		"tslib": "2.4.1",
		"typescript": "4.9.4",
		"vite": "^4.0.1",
		"vite-imagetools": "^4.0.11",
		"vitest": "^0.25.8"
	},
	"engines": {
		"node": ">=16.15.0",
		"npm": ">=8.10.0"
	}
}